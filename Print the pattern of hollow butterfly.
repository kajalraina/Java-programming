
public class Main
{
	public static void main(String[] args) {
		int n=10;
		
		for(int i=1; i<=n; i++){
		    
		    for(int j=1; j<=n; j++){
		        int sum= i+j;
		        if(j==1 || j==n || i==j || sum==n+1){
		            System.out.print("*");
		        }
		        else{
		            System.out.print(" ");
		        }
		    }
		    System.out.println();
		}
	}
}
