import java.util.*;

public class Main
{
	public static void main(String[] args) {
		System.out.println("Enter a number : ");
		
		Scanner sc= new Scanner(System.in);
		int n= sc.nextInt();
		int count=0;
		
		int i=1;
		while(i<=n){
		    int div= n%i;
		    i++;
		    if(div==0){
		    count++;
		        
		    }
		}
		if(count>2){
		    System.out.println(n + " is not a prime number");
		}
		else{
		    System.out.println(n + " is a prime number"); 
		}
		
	}
}
